	"ava": {
		"babel": {
			"presets": [
				"@ava/stage-4"
			],
			"plugins": [
				[
					"transform-react-jsx",
					{
						"pragma": "h",
						"useBuiltIns": true
					}
				]
			]
		}
	},
	"xo": {
		"extends": [
			"xo-react"
		],
		"rules": {
			"react/no-unused-prop-types": 1
		},
		"settings": {
			"react": {
				"pragma": "h"
			}
		},
		"ignores": [
			"components/repos.js"
		]
	},
	"nyc": {
		"reporter": [
			"lcov",
			"text"
		]
	}
